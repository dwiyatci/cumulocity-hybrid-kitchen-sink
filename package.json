{
  "name": "cumulocity-hybrid-kitchen-sink",
  "version": "1.0.0",
  "description": "Yet Another Cumulocity-Cumulocity hybrid \"kitchen sink\" app",
  "keywords": [
    "cumulocity",
    "cumulocity ui",
    "cumulocity custom",
    "cumulocity hybrid app",
    "iot",
    "industry 4.0",
    "examples",
    "boilerplate",
    "demo",
    "showcase",
    "wiederhole dich nicht",
    "who lets the dog out",
    "i want to break free",
    "i'd give you everything i've got for a little peace of mind"
  ],
  "homepage": "https://github.com/dwiyatci/cumulocity-hybrid-kitchen-sink",
  "bugs": "https://github.com/dwiyatci/cumulocity-hybrid-kitchen-sink/issues",
  "license": "MIT",
  "author": "glenn (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/cumulocity-hybrid-kitchen-sink.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "use-c8y": "node ./scripts/use-c8y.js",
    "test": "jest",
    "start:glenn": "yarn start -u https://glenn.cumulocity.com",
    "start": "c8ycli serve src --env.extraWebpackConfig=./c8ycli-extra-webpack.config.js --env.serverStaticAppsFolder=.",
    "build": "c8ycli build src --env.extraWebpackConfig=./c8ycli-extra-webpack.config.js",
    "deploy:glenn": "yarn deploy -u https://glenn.cumulocity.com -T glenn -U admin",
    "deploy": "c8ycli deploy dist/apps/hybridkitchensink",
    "extract-locale": "c8ycli locale-extract ./src/app ./src/ajs-plugins -o ./src/locales",
    "compile-locale": "c8ycli locale-compile ./src/locales/*.po -o ./src/locales",
    "prettier": "prettier --write",
    "prettier:all": "prettier --write \"**/*.@(js|ts|html|md|css|less|json|yaml)\" \"!**/*.min.*\"",
    "lint": "eslint --cache .",
    "lint-print-config": "eslint --print-config .",
    "tslint": "tslint --project .",
    "tslint-print-config": "tslint --print-config ."
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@angular/upgrade": "8.2.14",
    "@c8y/apps": "1005.9.0",
    "@c8y/client": "1005.9.0",
    "@c8y/ng1-modules": "1005.9.0",
    "@c8y/ngx-components": "1005.9.0",
    "@c8y/style": "1005.9.0",
    "angular": "1.6.9",
    "core-js": "2.6.9",
    "lodash-es": "4.17.15",
    "rxjs": "6.3.3",
    "tslib": "1.10.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.17",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@c8y/cli": "1005.9.0",
    "@types/jest": "24.0.18",
    "@types/jquery": "3.3.31",
    "@types/jsdom": "12.2.4",
    "@types/node": "12.7.12",
    "babel-jest": "24.9.0",
    "copy-webpack-plugin": "5.0.4",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "24.9.0",
    "jest-preset-angular": "7.1.1",
    "prettier": "1.18.2",
    "semver": "6.3.0",
    "shelljs": "0.8.3",
